{
  "$schema": "https://schema.tauri.app/config/2/capability",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "shell:allow-open",
    {
      "identifier": "http:default",
      "allow": [
        { "url": "https://www.inunison.dev/*" },
        { "url": "https://*.ably.io/*" },
        { "url": "https://*.ably-realtime.com/*" },
        { "url": "wss://*.ably-realtime.com/*" }
      ]
    }
  ]
}
